#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int N;
    std::vector<int> vec;
    int X;
    cin >> N;
    for (int i = 0; i < N; i++) {
        cin >> X;
        vec.push_back(X);
    }
    int Q;
    cin >> Q;
    int Y;
    for (int i = 0; i < Q; i++) {
        cin >> Y;
        for (int j = 0; j < N; j++) {
            if(vec[j] == Y){
                printf("Yes %d\n", j + 1);
                break;
            }else if(Y < vec[j]){
                printf("No %d\n", j + 1);
                break;
            }
        }
    }
    return 0;
}
